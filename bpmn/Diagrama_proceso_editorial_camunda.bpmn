<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_Editorial_v5" targetNamespace="http://example.com/bpmn/editorial" exporter="Camunda Modeler" exporterVersion="5.43.1">
  <bpmn:collaboration id="Collaboration_Editorial_v5">
    <bpmn:participant id="Participant_ProcesoEditorial_v5" name="Proceso editorial" processRef="Process_Editorial_v5" />
  </bpmn:collaboration>
  <bpmn:process id="Process_Editorial_v5" name="Proceso editorial de publicación" isExecutable="true" camunda:historyTimeToLive="P30D">
    <bpmn:laneSet id="LaneSet_Editorial_v5">
      <bpmn:lane id="Lane_Autor" name="Autor">
        <bpmn:flowNodeRef>StartEvent_Inicio</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>UserTask_Borrador</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>UserTask_EnviarRevision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>UserTask_CorregirReenviar</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Revisor" name="Revisor">
        <bpmn:flowNodeRef>UserTask_RevisionContenido</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Editor" name="Editor">
        <bpmn:flowNodeRef>UserTask_DecisionEditorial</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Decision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>UserTask_NotificarRechazo</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Rechazado</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>UserTask_PrepararPublicacion</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>UserTask_SolicitarCambios</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_Publicado</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>UserTask_Publicar</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_Inicio" name="Inicio">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:endEvent id="EndEvent_Rechazado" name="Rechazado">
      <bpmn:incoming>Flow_11</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:userTask id="UserTask_Borrador" name="Redactar borrador (DRAFT)">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="UserTask_EnviarRevision" name="Enviar a revisión">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="UserTask_CorregirReenviar" name="Corregir y reenviar">
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="UserTask_RevisionContenido" name="Revisión del contenido (IN_REVIEW)">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="UserTask_DecisionEditorial" name="Decisión editorial">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_Decision" name="¿Aprobar / Cambios / Rechazar?" default="Flow_10">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:userTask id="UserTask_NotificarRechazo" name="Notificar rechazo">
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="UserTask_PrepararPublicacion" name="Preparar publicación (APPROVED)">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:userTask id="UserTask_SolicitarCambios" name="Solicitar cambios">
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_Inicio" targetRef="UserTask_Borrador" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="UserTask_Borrador" targetRef="UserTask_EnviarRevision" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="UserTask_EnviarRevision" targetRef="UserTask_RevisionContenido" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="UserTask_RevisionContenido" targetRef="UserTask_DecisionEditorial" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="UserTask_DecisionEditorial" targetRef="Gateway_Decision" />
    <bpmn:sequenceFlow id="Flow_10" name="Rechazar" sourceRef="Gateway_Decision" targetRef="UserTask_NotificarRechazo" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="UserTask_NotificarRechazo" targetRef="EndEvent_Rechazado" />
    <bpmn:sequenceFlow id="Flow_6" name="Aprobar" sourceRef="Gateway_Decision" targetRef="UserTask_PrepararPublicacion">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${!requiereCambios &amp;&amp; aprobado}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_7" sourceRef="UserTask_PrepararPublicacion" targetRef="UserTask_Publicar" />
    <bpmn:sequenceFlow id="Flow_9" sourceRef="UserTask_Publicar" targetRef="EndEvent_Publicado" />
    <bpmn:sequenceFlow id="Flow_13" name="Cambios" sourceRef="Gateway_Decision" targetRef="UserTask_SolicitarCambios">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${requiereCambios}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_14" name="Corregir" sourceRef="UserTask_SolicitarCambios" targetRef="UserTask_CorregirReenviar" />
    <bpmn:sequenceFlow id="Flow_12" name="Reenviar" sourceRef="UserTask_CorregirReenviar" targetRef="UserTask_EnviarRevision" />
    <bpmn:endEvent id="EndEvent_Publicado" name="Publicado">
      <bpmn:incoming>Flow_9</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:userTask id="UserTask_Publicar" name="Publicar (PUBLISHED)">
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:userTask>
  </bpmn:process>
